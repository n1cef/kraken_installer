    }
    QTabBar::tab:hover {
        background: #2d2d39;
    }
    
    /* Group Boxes */
    QGroupBox {
        color: #64d2ff;
        font-size: 16px;
        border: 2px solid #4fc3f7;
        border-radius: 5px;
        margin: 10px;
        padding-top: 25px;
    }
    QGroupBox::title {
        subcontrol-origin: margin;
        left: 10px;
        padding: 0 5px;
    }
    
    /* Checkboxes */
    QCheckBox {
        color: #a0a0c0;
        spacing: 8px;
        font-size: 14px;
        margin: 4px 0;
    }
    QCheckBox::indicator {
        width: 20px;
        height: 20px;
        border: 2px solid #4fc3f7;
        border-radius: 4px;
    }
    QCheckBox::indicator:checked {
        background-color: #4fc3f7;
    }
    QCheckBox:hover {
        color: #ffffff;
    }
    
    /* Tool Box */
    QToolBox::tab {
        background: #1e1e1e;
        color: #a0a0c0;
        padding: 12px;
        border: 1px solid #363644;
        border-radius: 5px;
        margin: 4px 0;
        font-size: 14px;
    }
    QToolBox::tab:selected {
        background: #363644;
        color: #4fc3f7;
        border-color: #4fc3f7;
    }
    QToolBox::tab:hover {
        background: #2d2d39;
    }
    
    /* Scroll Bars */
    QScrollBar:vertical {
        background: #1e1e1e;
        width: 10px;
        border-radius: 5px;
    }
    QScrollBar::handle:vertical {
        background: #4fc3f7;
        min-height: 20px;
        border-radius: 5px;
    }
    QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
        border: none;
        background: none;
    }
    
    /* Buttons */
    QPushButton {
        color: #64d2ff;
        background-color: rgba(30, 30, 30, 0.7);
        border: 2px solid #4fc3f7;
        border-radius: 5px;
        padding: 10px 30px;
        font-size: 16px;
